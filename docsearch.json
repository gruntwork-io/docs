{
  "index_name": "docs_stage",
  "sitemap_urls": ["http://host.docker.internal:3000/sitemap.xml"],
  "sitemap_alternate_links": true,
  "strip_chars": " .,;:#",
  "selectors": {
    "lvl0": {
      "selector": "//nav[contains(@class,'navbar')]//a[contains(@class, 'navbar__link--active')]/text()",
      "type": "xpath",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": {
      "selector": "//nav[contains(@class,'menu')]/ul/li/a[contains(@class, 'active')]/text()",
      "type": "xpath",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl2": {
      "selector": "//nav[contains(@class,'menu')]/ul/li/ul/li/a[contains(@class, 'active')]/text()",
      "type": "xpath",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl3": {
      "selector": "//nav[contains(@class,'menu')]/ul/li/ul/li/ul/li/a[contains(@class, 'active')]/text()",
      "type": "xpath",
      "global": true
    },
    "lvl4": {
      "selector": "//nav[contains(@class,'menu')]/ul/li/ul/li/ul/li/ul/li/a[contains(@class, 'active')]/text()",
      "type": "xpath",
      "global": true
    },
    "content": "article p, article li, article td:last-child"
  },
  "custom_settings": {
    "separatorsToIndex": "_",
    "attributesForFaceting": ["language", "version", "type", "docusaurus_tag"],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  }
}
